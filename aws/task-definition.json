{
  "containerDefinitions": [ 
   { 
     "essential": true,
     "image": "224931607066.dkr.ecr.ap-southeast-1.amazonaws.com:%BUILD_NUMBER%",
     "memoryReservation": 1024,
     "environment": [
      {
       "name": "NODE_ENV",
       "value": "%ENV%"
      }
     ],
     "logConfiguration": { 
      "logDriver": "awslogs",
      "options": { 
        "awslogs-group" : "/ecs/dev-defination",
        "awslogs-region": "AWS_REGION",
        "awslogs-stream-prefix": "ecs"
      }
     },
     "name": "backend",
     "portMappings": [ 
      { 
        "containerPort": 80,
        "hostPort": 80,
        "protocol": "tcp"
      }
     ]
   }
  ],
  "cpu": "512",
  "executionRoleArn": "dev-taskexecutionrole",
  "family": "dev-defination",
  "taskRoleArn": "dev-taskrole",
  "memory": "1024",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [ 
    "FARGATE" 
  ]
}
